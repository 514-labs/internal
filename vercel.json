{
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "deploymentProtection": {
    "bypassForRoutes": [
      {
        "path": "/.well-known/*",
        "methods": ["GET", "OPTIONS"]
      },
      {
        "path": "/mcp",
        "methods": ["GET", "POST", "OPTIONS"]
      }
    ]
  }
}
